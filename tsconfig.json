{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "types": [
      "node",
      "jasmine"
    ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "awesomeTypeScriptLoaderOptions": {
    "useWebpackText": true
  },
  "exclude": [
    "node_modules"
  ]
}
